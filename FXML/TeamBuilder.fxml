<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<AnchorPane id="StackPane" prefHeight="720.0" prefWidth="1280.0"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="fochamon.Controllers.TeamBuilder">
    <VBox prefWidth="1280.0" prefHeight="660.0"
    	AnchorPane.topAnchor="0" AnchorPane.leftAnchor="0">
        
        <GridPane prefWidth="1280.0" prefHeight="660.0">
            
            <columnConstraints>
                <ColumnConstraints percentWidth="33"/>
                <ColumnConstraints percentWidth="34"/>
                <ColumnConstraints percentWidth="33"/>
            </columnConstraints>

            <rowConstraints>
                <RowConstraints percentHeight="50"/>
                <RowConstraints percentHeight="50"/>
            </rowConstraints>
            
            <VBox spacing="10" alignment="CENTER" 
                  GridPane.columnIndex="0" GridPane.rowIndex="0"
                  GridPane.rowSpan="2"
                  style="-fx-background-color: #00F;">
                  
            </VBox>
			<VBox spacing="10" alignment="CENTER" 
                  GridPane.columnIndex="1" GridPane.rowIndex="0"
                  style="-fx-background-color: #F00;">
            </VBox>
			<VBox spacing="10" alignment="CENTER" 
                  GridPane.columnIndex="2" GridPane.rowIndex="0"
                  style="-fx-background-color: #0FF;">
            </VBox>
            <VBox spacing="10" alignment="CENTER" 
                  GridPane.columnIndex="1" GridPane.rowIndex="1"
                  GridPane.columnSpan="2"
                  style="-fx-background-color: #0F0;">
            </VBox>
        </GridPane>
    </VBox>
    
    <HBox alignment="CENTER" spacing="8"
    	AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0">
        <Button text="Back to ze accueil"
        	styleClass="secondary" onAction="#back"/>
    </HBox>
    
</AnchorPane>