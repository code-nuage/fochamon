<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<AnchorPane id="StackPane" prefHeight="720.0" prefWidth="1280.0" xmlns:fx="http://javafx.com/fxml" fx:controller="fochamon.Controllers.Home">
    <VBox alignment="CENTER" spacing="16" prefWidth="1280.0" prefHeight="720.0">
        <ImageView>
            <image>
                <Image url="@fochamon-logo-upscaled.png"/>
            </image>
        </ImageView>

        <VBox alignment="CENTER" spacing="8">
            <Button text="Construction de tchim" styleClass="primary" onAction="#teambuilder"/>
            <Button text="Flight" styleClass="primary" onAction="#fight"/>
            <Button text="Return to ze bureau" styleClass="secondary" onAction="#quit"/>
        </VBox>
    </VBox>
</AnchorPane>
